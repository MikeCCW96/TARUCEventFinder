<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:paddingLeft="13dp"
        android:id="@+id/textViewStartDate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:textSize="20dp"
        android:text="startDate"
        android:textColor="@color/colorPrimary"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textViewEndDate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="13dp"
        android:text="endDate"
        android:textSize="20dp"
        android:layout_alignBaseline="@+id/textViewStartDate"
        android:layout_alignBottom="@+id/textViewStartDate"
        android:layout_toEndOf="@+id/textViewId"
        android:layout_marginStart="180dp" />

    <TextView
        android:paddingLeft="13dp"
        android:id="@+id/textViewStartTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:text="startTime"
        android:textSize="15dp"
        android:textColor="@color/colorPrimary"
        android:layout_below="@+id/textViewStartDate"
        android:layout_alignParentStart="true"
        android:paddingBottom="15dp"/>




    <TextView
        android:id="@+id/textViewEndTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="15dp"
        android:paddingLeft="13dp"
        android:text="endTime"
        android:textSize="15dp"
        android:layout_alignBaseline="@+id/textViewStartTime"
        android:layout_alignBottom="@+id/textViewStartTime"
        android:layout_alignStart="@+id/textViewEndDate" />

    <TextView
        android:id="@+id/textViewId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textViewStartDate"
        android:text="TextView"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/textViewVenueName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="15dp"
        android:paddingLeft="13dp"
        android:text="venueName"
        android:textColor="@color/colorPrimary"
        android:textSize="15dp"
        android:layout_below="@+id/textViewStartTime"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/textViewVenueDescription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="15dp"
        android:paddingLeft="13dp"
        android:text="venueDescription"
        android:textColor="@color/colorPrimary"
        android:textSize="15dp"
        android:layout_below="@+id/textViewVenueName"
        android:layout_toEndOf="@+id/textViewId" />

    <TextView
        android:id="@+id/TextViewTimeTableId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewVenueDescription"
        android:layout_alignStart="@+id/textViewEndTime"
        android:layout_marginStart="38dp"
        android:text="timeTableId"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/TextViewWaitingListStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewVenueDescription"

        android:layout_marginStart="38dp"
        android:text="waitingListStatus"
        android:visibility="gone"/>
</RelativeLayout>